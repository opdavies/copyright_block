<?php

/**
 * @file
 * Install file for "Copyright block module".
 */

/**
 * Implements hook_uninstall().
 */
function copyright_block_module_uninstall() {
  variable_del('copyright_block_module_inceptionyear');
  variable_del('copyright_block_module_copyrightholder');

  db_delete('block')->condition('module', 'copyright_block_module')->condition('delta', 'copyright')->execute();
}
